{
  "name": "entertainment-music",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "type:check": "tsc -w",
    "build": "tsc && vite build",
    "postbuild": "pnpm run cleanup",
    "preview": "vite preview",
    "prettier:check": "prettier --config .prettierrc.json --check 'src/**/*.{ts, .tsx, scss}'",
    "prettier:fix": "pnpm dlx prettier --write 'src/**/*.{ts, .tsx, scss}'",
    "stylelint:check": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint --fix src/**/*.scss",
    "eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.cjs src",
    "dev": "vite",
    "cleanup": "pnpm dlx rimraf dist/*.map"
  },
  "dependencies": {
    "@mui/base": "*",
    "@mui/icons-material": "*",
    "@mui/lab": "*",
    "@mui/material": "*",
    "@mui/styles": "*",
    "@mui/system": "*",
    "@mui/x-date-pickers-pro": "*",
    "@mui/x-license-pro": "*",
    "@vitejs/plugin-react": "^4.3.2",
    "clsx": "*",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "tailwind-merge": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/autosuggest-highlight": "*",
    "@types/dompurify": "^3.0.2",
    "@types/node": "^18.15.5",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "*",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "autosuggest-highlight": "3.3.4",
    "eslint": "8.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "globals": "^15.10.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "rimraf": "^5.0.1",
    "sass": "^1.58.1",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-scss": "^4.5.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.8.0",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-sass": "^0.1.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@8.15.8"
}
